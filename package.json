{
  "name": "piyango-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c tslint.json \"api/**/*.ts\" \"src/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "prod": "now --prod",
    "rmtmp": "rm -rf /tmp/zeit-fun-*",
    "sync:draws": "ts-node scripts/syncDraws.ts",
    "sync:commit": "ts-node scripts/syncDrawsAndCommit.ts",
    "docs": "ts-node scripts/generateDocs.ts && prettier --write index.html"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "echo $HUSKY_GIT_PARAMS",
      "commit-msg": "echo $HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@now/node": "^1.4.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "node-fetch": "^2.6.0",
    "strip-bom": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/markdown-it": "^0.0.9",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^13.9.0",
    "@types/node-fetch": "^2.5.5",
    "husky": "^4.2.3",
    "markdown-it": "^10.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.2.0",
    "typescript": "^3.8.3"
  }
}
