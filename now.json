{
  "version": 2,
  "env": {},
  "alias": ["piyango"],
  "routes": [
    {
      "src": "/api.*",
      "headers": { "Cache-Control": "s-maxage=86400" },
      "continue": true
    },
    {
      "src": "/draws.*",
      "headers": {
        "Cache-Control": "s-maxage=31536000",
        "Content-Type": "application/json"
      },
      "continue": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
