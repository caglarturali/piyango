{
  "version": 2,
  "env": {},
  "alias": ["piyango"],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "X-XSS-Protection": "1; mode=block"
      },
      "continue": true
    },
    {
      "src": "/api.*",
      "headers": { "Cache-Control": "s-maxage=86400" },
      "continue": true
    },
    {
      "src": "/draws.*",
      "headers": {
        "Cache-Control": "s-maxage=31536000",
        "Content-Type": "application/json"
      },
      "continue": true
    }
  ]
}
