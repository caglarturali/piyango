{
  "name": "@caglarturali/piyango-utils",
  "version": "1.5.1",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "repository": "git://github.com:caglarturali/piyango.git",
  "scripts": {
    "clean": "rimraf ./cjs ./esm",
    "prebuild": "pnpm run clean",
    "build": "pnpm run build:cjs && pnpm run build:esm",
    "build:cjs": "tsc -p tsconfig.build.json --module commonjs --outDir cjs",
    "build:esm": "tsc -p tsconfig.build.json --module esnext --outDir esm",
    "prepack": "pnpm run build",
    "sloc": "node-sloc . -x node_modules,cjs,esm"
  },
  "dependencies": {
    "@caglarturali/piyango-common": "^8.1.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC"
}
